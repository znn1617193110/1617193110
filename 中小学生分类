class HighschoolStudent(Student):
   primarystuCount = 0
                     
   def __init__(self,name,stu_no,stu_class,male,gender):
      self.name = name
      self.stu_no = stu_no
      self.stu_class = stu_class
      self.male = male
      self.gender = gender
      Student.stuCount += 1
      PrimaryStudent.primarystuCount += 1
   def Recite(self):
      print "High school Student can recite"
 
   def Oral(self):
      print "High school Student can oral"

   def Write(self):
      print "High school Student can write" 
 
 
